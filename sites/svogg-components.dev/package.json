{
	"name": "svogg-components.dev",
	"version": "0.5.3",
	"private": true,
	"description": "Documentation for svogg-components.",
	"author": "MikaelJohnAndersson <mikael.andersson@moggstudio.se>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/MikaelJohnAndersson/svogg-components.git"
	},
	"scripts": {
		"dev": "vite dev",
		"dev:expose": "vite dev --host --port 3333",
		"build": "vite build",
		"build:workspace": "pnpm -w run svogg-components package & vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@iconify/svelte": "^3.1.4",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.20.4",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"highlight.js": "^11.9.0",
		"postcss": "^8.4.31",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"sveld": "^0.18.1",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"svelte-highlight": "^7.4.1",
		"svogg-components": "workspace:*",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2",
		"vite-plugin-sveld": "^1.1.0"
	},
	"type": "module"
}
